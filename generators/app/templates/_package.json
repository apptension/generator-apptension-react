{
  "name": "<%= appName %>",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean-dist start-dev-server",
    "start:dev-opt": "npm-run-all clean-dist start-dev-opt-server",
    "build": "npm-run-all clean-dist webpack:prod",
    "test": "mocha --compilers js:babel-core/register app/spec/test.index.js --recursive \"./app/**/*.spec.js\"",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha -x **/*.spec.js -- --compilers js:babel-core/register --colors --bail --reporter dot app/spec/test.index.js --recursive \"./app/**/*.spec.js\"",
    "clean-dist": "rimraf ./dist/*",
    "lint": "eslint app --ext .js,.jsx",
    "start-dev-server": "babel-node bin/startDevServer",
    "start-dev-opt-server": "babel-node bin/startOptimizedDevServer",
    "webpack:prod": "webpack --config config/webpack/webpack-prod.config.babel.js",
    "webpack:dev": "webpack --config config/webpack/webpack-dev.config.babel.js",
    "webpack:dev-opt": "webpack --config config/webpack/webpack-dev-opt.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "<%= author %>",
  "license": "<%= license %>",
  "dependencies": {
    "apptension-tools": "6.0.0-alpha3",
    "babel": "^6.5.2",
    "babel-cli": "6.18.0",
    "babel-core": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "6.18.0",
    "babylonjs": "^2.4.1",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "eslint-config-apptension": "^1.0.9",
    "eslint-plugin-react": "6.4.1",
    "exports-loader": "^0.6.2",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "istanbul": "1.1.0-alpha.1",
    "jsdom": "9.8.3",
    "lodash": "^4.2.1 ",
    "mocha": "^3.1.2",
    "npm-run-all": "^3.1.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-ga": "^2.1.2",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.5",
    "react-router": "3.0.0",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-api-middleware": "^1.0.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6"
  }
}
